@model IEnumerable<string>

<a class="btn btn-block btn-secondary categoryBtn" asp-action="Index" asp-controller="Home" asp-route-category="">
    Home
</a>

@foreach (string category in Model) 
{
    <a class="btn btn-block btn-secondary categoryBtn" asp-action="Index" 
       asp-controller="Home" asp-route-category="@category">
        @category
    </a>
}